<h1>Editing project</h1>

<%= error_messages_for :project %>
<% form_for(@project) do |f| %>
  <p>
    <label for="name">Project Name</label><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @project %> |
<%= link_to 'Back', projects_path %>