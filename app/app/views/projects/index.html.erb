<div id="index">
  <h1>Current projects</h1>
  <table class="project">
    <tr>
      <th>Project Title</th>
      <th># Of Issues</th>
      <th>Status</th>
      <th>Launch Date</th>
      <% if logged_in? -%>
        <th></th>
      <% end -%>
    </tr>
    <%= render :partial => 'project', :collection => @projects %>
  </table>
</div>